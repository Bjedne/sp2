{
  "name": "semester-project-2",
  "version": "0.1.3",
  "description": "npm package for semester project 2 containing bootstrap, ESLint and Prettier as dependecies",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "watch": "sass src/scss:dist/css --watch",
    "start": "live-server",
    "dev": "npm-run-all --parallel watch start",
    "test": "node src/index.js",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bjedne/sp2.git"
  },
  "keywords": [
    "auction"
  ],
  "author": "Bj√∏rnar Jakobsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bjedne/sp2/issues"
  },
  "homepage": "https://github.com/Bjedne/sp2#readme",
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "eslint": "^9.1.1",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "live-server": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.76.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "bootstrap": "^5.2.0"
  }
}
